        int[] array = new int[10];


        array[0] = 5;//1--
        array[1] = 4;//2
        array[2] = 9;//3--
        array[3] = 10;//4
        array[4] = 1;//5--
        array[5] = 5;//6
        array[6] = 4;//7--
        array[7] = 9;//8
        array[8] = 10;//9++++elementna tpum-
        array[9] = 10;//10

        int sum = 0;
        int index =0;//01
        for (int i = 0; i < array.length; i++) {//1.2.3.4
            if ( array[i] % 2 == 0){//array [i]5.4.9
                index = array[i];
                index+=array[i];

            }

        }


////5 2 3 11 5
        //13 5 5

        System.out.println(sum);
        System.out.println(index);
    }
}

