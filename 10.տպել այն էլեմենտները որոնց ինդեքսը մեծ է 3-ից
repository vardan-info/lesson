public class Main {
    public static void main(String[] args) {
          int arr[] = {10,1,2,3,12,13};
        int sum=0;
        int index =0;
        for (int i = 0; i <6; i++) {
            if (i > 3) {//0.1.2.3.4.5.6

                sum+=arr[i];


            }



        } System.out.println(sum);
    }
}
